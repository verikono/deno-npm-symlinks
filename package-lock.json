{
    "name": "basic-npm-monorepo",
    "version": "1.0.0",
    "lockfileVersion": 3,
    "requires": true,
    "packages": {
        "": {
            "name": "basic-npm-monorepo",
            "version": "1.0.0",
            "workspaces": [
                "packages/*"
            ],
            "dependencies": {
                "main-project": "*",
                "pkg-a": "*",
                "pkg-b": "*"
            }
        },
        "node_modules/main-project": {
            "resolved": "packages/main-project",
            "link": true
        },
        "node_modules/pkg-a": {
            "resolved": "packages/pkg-a",
            "link": true
        },
        "node_modules/pkg-b": {
            "resolved": "packages/pkg-b",
            "link": true
        },
        "packages/main-project": {
            "version": "1.0.0",
            "dependencies": {
                "pkg-a": "*",
                "pkg-b": "*"
            }
        },
        "packages/pkg-a": {
            "version": "1.0.0",
            "dependencies": {
                "pkg-b": "*"
            }
        },
        "packages/pkg-b": {
            "version": "1.0.0",
            "dependencies": {
                "pkg-a": "*"
            }
        }
    }
}
